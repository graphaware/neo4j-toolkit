<?xml version="1.0"?>
<phpunit bootstrap="./tests/bootstrap.php" colors="true" verbose="true">
    <testsuites>
        <testsuite name="NeoToolkit TestSuite">
            <directory>./tests</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist>
            <directory suffix=".php">./src</directory>
        </whitelist>
    </filter>
</phpunit>
